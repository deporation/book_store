<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.labservice.demo.mapper.ProjectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.labservice.demo.entity.Project">
        <id column="pnum" property="pnum" />
        <result column="proname" property="proname" />
        <result column="sid" property="sid" />
        <result column="tid" property="tid" />
        <result column="status" property="status" />
        <result column="lid" property="lid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pnum, proname, sid, tid, status, lid
    </sql>

</mapper>
