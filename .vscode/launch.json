    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CodeLens (Launch) - Edit",
            "request": "launch",
            "mainClass": "com.labservice.demo.util.Edit",
            "projectName": "demo"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-DemoApplication<demo>",
            "request": "launch",
            "mainClass": "com.labservice.demo.DemoApplication",
            "projectName": "demo"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}//src//main//resources//templates",
            "sourceMaps": true
        }
    ]
}